<?xml version="1.0" encoding="utf-8"?>
<GF_TREINAMENTO>
<DATABASE TITLE="Treinamento" NAME="Treinamento">
	<TABLE TITLE="SYS~Sequencial" NAME="SYS~Sequencial">
		<FIELD TYPE="TEXT" SIZE="10" TITLE="PW~Projeto" NAME="PW~Projeto" />
		<FIELD TYPE="TEXT" SIZE="30" TITLE="SYS~Tabela" NAME="SYS~Tabela" />
		<FIELD TYPE="TEXT" SIZE="30" TITLE="SYS~Campo" NAME="SYS~Campo" />
		<FIELD TYPE="TEXT" SIZE="50" TITLE="SYS~Valor" NAME="SYS~Valor" />
		<FIELD TYPE="TEXT" SIZE="50" TITLE="SYS~ValorAnterior" NAME="SYS~ValorAnterior" />
		<FIELD TYPE="TEXT" SIZE="50" TITLE="SYS~Estacao" NAME="SYS~Estacao" />
		<FIELD TYPE="TEXT" SIZE="30" TITLE="SYS~Identificacao" NAME="SYS~Identificacao" />
		<FIELD TYPE="NUMBER" SIZE="6" MASK="999999" INTERNAL_SEQUENCE="FALSE" DEFAULT="0" TITLE="SYS~Pendentes" NAME="SYS~Pendentes" />
		<INDEX TITLE="Chave sequencial" NAME="Chave sequencial" PRIMARY="True" UNIQUE="True" ISCLUSTER="True" FILLFACTOR="90">
			<FIELD NAME="SYS~Tabela" />
			<FIELD NAME="SYS~Campo" />
		</INDEX>
	</TABLE>
	<TABLE TITLE="Carteira" NAME="TB_CARTEIRA">
		<FIELD TYPE="NUMBER" SIZE="10" MASK="9999999999" INTERNAL_SEQUENCE="TRUE" SEQUENCE="1" TITLE="ID da carteira" NAME="CAR_ID" />
		<FIELD TYPE="TEXT" SIZE="100" TITLE="Nome da Carteira" NAME="CAR_NOME" />
		<INDEX TITLE="PK_TB_CARTEIRA" NAME="PK_TB_CARTEIRA" PRIMARY="True" UNIQUE="True" ISCLUSTER="True" FILLFACTOR="90">
			<FIELD NAME="CAR_ID" />
		</INDEX>
	</TABLE>
	<TABLE TITLE="Centro de Custo" NAME="TB_CENTRO_DE_CUSTO">
		<FIELD TYPE="NUMBER" SIZE="10" MASK="9999999999" INTERNAL_SEQUENCE="TRUE" SEQUENCE="1" TITLE="ID Centro de Custo" NAME="CC_ID" />
		<FIELD TYPE="TEXT" SIZE="100" TITLE="Nome do centro de custos" NAME="CC_NOME" />
		<INDEX TITLE="PK_TB_CENTRO_DE_CUSTO" NAME="PK_TB_CENTRO_DE_CUSTO" PRIMARY="True" UNIQUE="True" ISCLUSTER="True" FILLFACTOR="90">
			<FIELD NAME="CC_ID" />
		</INDEX>
	</TABLE>
	<TABLE TITLE="Conta" NAME="TB_CONTA">
		<FIELD TYPE="TEXT" SIZE="10" SEQUENCE="1" TITLE="ID da Conta" NAME="CT_ID" />
		<FIELD TYPE="TEXT" SIZE="100" TITLE="Nome da conta" NAME="CT_NOME" />
		<FIELD TYPE="NUMBER" SIZE="10" MASK="9999999999" INTERNAL_SEQUENCE="FALSE" DEFAULT="0" TITLE="ID do Grupo" NAME="GC_ID" />
		<INDEX TITLE="PK_TB_CONTA" NAME="PK_TB_CONTA" PRIMARY="True" UNIQUE="True" ISCLUSTER="True" FILLFACTOR="90">
		</INDEX>
	</TABLE>
	<TABLE TITLE="Grupo de conta" NAME="TB_GRUPO_CONTA">
		<FIELD TYPE="NUMBER" SIZE="10" MASK="9999999999" INTERNAL_SEQUENCE="TRUE" SEQUENCE="1" DEFAULT="0" TITLE="ID do Grupo" NAME="GC_ID" />
		<FIELD TYPE="TEXT" SIZE="100" TITLE="Nome do Grupo de conta" NAME="GC_NOME" />
		<INDEX TITLE="PK_TB_GRUPO_CONTA" NAME="PK_TB_GRUPO_CONTA" PRIMARY="True" UNIQUE="True" ISCLUSTER="True" FILLFACTOR="90">
			<FIELD NAME="GC_ID" />
		</INDEX>
	</TABLE>
	<TABLE TITLE="Grupo de login" NAME="TB_LOGIN_GROUP">
		<FIELD TYPE="TEXT" SIZE="60" MASK="@!" TITLE="Nome do grupo" NAME="LOGIN_GROUP_NAME" />
		<FIELD TYPE="BOOLEAN" TITLE="Grupo tipo administração" NAME="LOGIN_GROUP_IS_ADMIN" />
		<INDEX TITLE="Por nome" NAME="LOGIN_GROUP_PK" PRIMARY="True" UNIQUE="True" ISCLUSTER="True" FILLFACTOR="90">
			<FIELD NAME="LOGIN_GROUP_NAME" />
		</INDEX>
		<RELATION FOREIGNTABLE="TB_LOGIN_RULE" BASETABLE="TB_LOGIN_GROUP" TYPE="CONSTRAINT" CARDINALITY="1-N" TITLE="Grupo para regra" NAME="TB_LOGIN_GROUP_RULE">
			<RELATIONOF FOREIGNFIELD="LOGIN_GROUP_NAME" BASEFIELD="LOGIN_GROUP_NAME" NAME="LOGIN_GROUP_NAME" />
		</RELATION>
		<RELATION FOREIGNTABLE="TB_LOGIN_USER" BASETABLE="TB_LOGIN_GROUP" TYPE="CONSTRAINT" CARDINALITY="1-N" TITLE="Grupo para usuário" NAME="TB_LOGIN_GROUP_USER">
			<RELATIONOF FOREIGNFIELD="LOGIN_GROUP_NAME" BASEFIELD="LOGIN_GROUP_NAME" NAME="LOGIN_GROUP_NAME" />
		</RELATION>
	</TABLE>
	<TABLE TITLE="Regras para login" NAME="TB_LOGIN_RULE">
		<FIELD TYPE="TEXT" SIZE="8" MASK="@!" TITLE="Id do projeto" NAME="LOGIN_RULE_PROJECT" />
		<FIELD TYPE="TEXT" SIZE="60" MASK="@!" TITLE="Nome do grupo" NAME="LOGIN_GROUP_NAME" />
		<FIELD TYPE="TEXT" SIZE="100" TITLE="Objeto da regra" NAME="LOGIN_RULE_OBJECT" />
		<FIELD TYPE="TEXT" SIZE="100" TITLE="Permissões da regra" NAME="LOGIN_RULE_PERMISSIONS" />
		<INDEX TITLE="Por projeto/grupo/objeto" NAME="LOGIN_RULE_PK" PRIMARY="True" UNIQUE="True" ISCLUSTER="True" FILLFACTOR="90">
			<FIELD NAME="LOGIN_RULE_PROJECT" />
			<FIELD NAME="LOGIN_GROUP_NAME" />
			<FIELD NAME="LOGIN_RULE_OBJECT" />
		</INDEX>
	</TABLE>
	<TABLE TITLE="Token para login" NAME="TB_LOGIN_TOKEN">
		<FIELD TYPE="TEXT" SIZE="36" MASK="@!" TITLE="Id do token" NAME="LOGIN_TOKEN_ID" />
		<FIELD TYPE="TEXT" SIZE="40" MASK="@!" TITLE="Id do usuário" NAME="LOGIN_USER_LOGIN" />
		<FIELD TYPE="DATETIME" MASK="dd/MM/yyyy HH:mm:ss" TITLE="Data de emissão do token" NAME="LOGIN_TOKEN_ACQUIRED" />
		<FIELD TYPE="DATETIME" MASK="dd/MM/yyyy HH:mm:ss" TITLE="Data de expiração do token" NAME="LOGIN_TOKEN_EXPIRATION" />
		<INDEX TITLE="Por token" NAME="LOGIN_TOKEN_PK" PRIMARY="True" UNIQUE="True" ISCLUSTER="True" FILLFACTOR="90">
			<FIELD NAME="LOGIN_TOKEN_ID" />
		</INDEX>
	</TABLE>
	<TABLE TITLE="Usuário de login" NAME="TB_LOGIN_USER">
		<FIELD TYPE="TEXT" SIZE="60" MASK="@!" TITLE="Nome do grupo" NAME="LOGIN_GROUP_NAME" />
		<FIELD TYPE="TEXT" SIZE="40" MASK="@!" TITLE="Login" NAME="LOGIN_USER_LOGIN" />
		<FIELD TYPE="TEXT" SIZE="40" MASK="@!" TITLE="Senha" NAME="LOGIN_USER_PASSWORD" />
		<FIELD TYPE="TEXT" SIZE="60" MASK="@!" TITLE="Nome do usuário" NAME="LOGIN_USER_NAME" />
		<FIELD TYPE="MEMO" MASK="@!" TITLE="Observações" NAME="LOGIN_USER_OBS" />
		<INDEX TITLE="Por login" NAME="LOGIN_USER_PK" PRIMARY="True" UNIQUE="True" ISCLUSTER="True" FILLFACTOR="90">
			<FIELD NAME="LOGIN_USER_LOGIN" />
		</INDEX>
		<RELATION FOREIGNTABLE="TB_LOGIN_TOKEN" BASETABLE="TB_LOGIN_USER" TYPE="CONSTRAINT" CARDINALITY="1-N" TITLE="Usuário para token" NAME="TB_LOGIN_USER_TOKEN">
			<RELATIONOF FOREIGNFIELD="LOGIN_USER_LOGIN" BASEFIELD="LOGIN_USER_LOGIN" NAME="LOGIN_USER_LOGIN" />
		</RELATION>
	</TABLE>
</DATABASE>
</GF_TREINAMENTO>
